<script type="text/javascript">
window.onload = function() {
	var v = document.getElementById('userName');
	if (v != null) {
		v.focus();
	}
	switcher("oTabList", "oTabDiv");
	switcher("oTab2List", "oTab2Div");
	switcher("aTabList", "aTabDiv", "aTabDivMore");
	switcher("bTabList", "bTabDiv", "bTabDivMore");
	switcher("cTabList", "cTabDiv", "cTabDivMore");
	loadFrameData("yjfk", "/minichan/all_yjfk.jsp");
	return;
};

function findPosX(obj) {
	var curleft = 0;
	if (obj.offsetParent)
		while(1) {
			curleft += obj.offsetLeft;
			if(!obj.offsetParent) break;
			obj = obj.offsetParent;
		}
	else if (obj.x)
		curleft += obj.x;
	return curleft;
}
function findPosY(obj) {
	var curtop = 0;
	if (obj.offsetParent)
		while(1) {
			curtop += obj.offsetTop;
			if(!obj.offsetParent) break;
			obj = obj.offsetParent;
		}
	else if(obj.y)
		curtop += obj.y;
	return curtop;
}
function showyjfk() {
	var img = document.getElementById('imgyjfk');
	var obj = document.getElementById('yjfk');
	if (obj.style) {
		obj.style.left=(findPosX(img)-0)+"px";
		obj.style.top=(findPosY(img)+img.height-0)+"px";
		obj.style.display='block';
	} else {
		obj.left=(findPosX(img)-0)+"px";
		obj.top=(findPosY(img)+img.height-0)+"px";
		obj.display='';
	}
	return false;
}
function hideyjfk() {
	var obj = document.getElementById('yjfk');
	if (obj.style) {
		obj.style.display='none';
	} else {
		obj.display='none';
	}
	return false;
}

/* 搜索框事件  */
function submitSearch(){
 	var getp = $("#searchQuery").val();
 	if("" == getp){
		$("#searchQuery").focus();
		$("#searchQuery").attr("placeholder","搜索词不能为空...");
		return;
	}
 	
	var getpar = escape(getp);
	window.open("http://166.111.6.39/web?query="+getpar+"&ie=utf8");
	 
}

function searchkeydown(event){
	if(event.keyCode ==13){
		submitSearch();
	  }
}
/* 搜索框事件 end */
	
